        <attr name="colorProjectAvailable" format="color" />
        <attr name="colorProjectValidated" format="color" />
        <attr name="colorProjectFailed" format="color" />
        <attr name="colorProjectInProgress" format="color" />
        <attr name="colorProjectOnUnavailable" format="color" />
        <attr name="colorProjectOnAvailable" format="color" />
        <attr name="colorProjectOnValidated" format="color" />
        <attr name="colorProjectOnFailed" format="color" />
        <attr name="colorProjectOnInProgress" format="color" />
